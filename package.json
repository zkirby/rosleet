{
  "name": "reetcode",
  "version": "1.0.0",
  "description": "Adds leetcode style in-browser editor to Rosalind",
  "private": false,
  "type": "module",
  "bin": {
    "reetcode": "./scripts/index.js"
  },
  "scripts": {
    "build": "rollup -c && node scripts/copy-to-clipboard.js",
    "build:nocp": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "rollup -c -w"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "clipboardy": "^4.0.0",
    "rollup": "^4.54.0",
    "rollup-plugin-string": "^3.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
